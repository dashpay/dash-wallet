<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Explore.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:background">@color/white</item>
        <item name="android:textColor">@color/gray_900</item>
    </style>

    <style name="DashButton.Blue.Small" parent="DashButton.Blue">
        <item name="android:background">@drawable/small_blue_button_background</item>
        <item name="fontFamily">@font/montserrat_semibold</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="DashButton.Green.Small" parent="DashButton.Blue">
        <item name="android:background">@drawable/small_green_button_background</item>
        <item name="fontFamily">@font/montserrat_semibold</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="ExploreTestBottomContentBackground" parent="WhiteBackground">
        <item name="cornerRadius">8dp</item>
    </style>

    <style name="ExploreBottomContentBackground">
        <item name="backgroundColor">@color/gray_40</item>
        <item name="strokeColor">@color/gray_40</item>
        <item name="cornerRadius">8dp</item>
    </style>

    <style name="ExploreSearchResultsBackground" parent="WhiteBackground">
        <item name="cornerRadius">8dp</item>
    </style>

    <style name="DialogIconBackground">
        <item name="backgroundColor">@color/blue_50</item>
        <item name="strokeColor">@color/blue_50</item>
        <item name="cornerRadius">25dp</item>
    </style>

    <style name="UpButtonBackground">
        <item name="backgroundColor">@color/gray_900_0.7</item>
        <item name="strokeColor">@android:color/transparent</item>
        <item name="cornerRadius">22dp</item>
    </style>

    <style name="FilterIconBackground">
        <item name="backgroundColor">@color/dash_blue</item>
        <item name="strokeColor">@color/dash_blue</item>
        <item name="cornerRadius">13dp</item>
    </style>
</resources>